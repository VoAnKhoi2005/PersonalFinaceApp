<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!--overall-->
    <SolidColorBrush Color="#E5CBBFBF" x:Key="CommonText"></SolidColorBrush>
    <SolidColorBrush Color="#053B50" x:Key="CommonGridBackground"></SolidColorBrush>
    <SolidColorBrush Color="#003E57" x:Key="DockpanelBackGround"></SolidColorBrush>
    <SolidColorBrush Color="#053B50" x:Key="onClicked"></SolidColorBrush>
    <SolidColorBrush Color="#265B70" x:Key="onHovered"></SolidColorBrush>
    <SolidColorBrush Color="DeepSkyBlue" x:Key="Underline"></SolidColorBrush>
    <SolidColorBrush Color="#0d2129" x:Key="Login_button"></SolidColorBrush>
    <SolidColorBrush Color="#084563" x:Key="Login_border"></SolidColorBrush>
    <SolidColorBrush Color="White" x:Key="Login_buttonText"></SolidColorBrush>

    <SolidColorBrush Color="#FF61D2BE" x:Key="Confirm_button"></SolidColorBrush>
    <SolidColorBrush Color="#FF6DF096" x:Key="Confirm_buttonHover"></SolidColorBrush>
    <SolidColorBrush Color="#FFC8386C" x:Key="Cancel_button"></SolidColorBrush>
    <SolidColorBrush Color="Red" x:Key="Cancel_buttonHover"></SolidColorBrush>
    <!--dashboard-->
    <SolidColorBrush Color="Black" x:Key="Dashboard_percent"></SolidColorBrush>
    <SolidColorBrush Color="#25FFAE" x:Key="Dashboard_trueColor"></SolidColorBrush>
    <SolidColorBrush Color="#FFF7413A" x:Key="Dashboard_falseColor"></SolidColorBrush>
    
    <!--expense book-->
    <SolidColorBrush Color="White" x:Key="ExpenseBook_Uparrow"></SolidColorBrush>
    <SolidColorBrush Color="Gray" x:Key="ExpenseBook_DownArrow"></SolidColorBrush>
        <!--addnew-->
    <SolidColorBrush Color="#FFD9D9D9" x:Key="ExpenseBook_addnew_backgroundBorder"></SolidColorBrush>
    
    <!--goalplan-->
        <!--card-->
    <SolidColorBrush Color="#FFF6E8A1" x:Key="Goalplan_Hover"></SolidColorBrush>
    
    <!--summary-->
    <SolidColorBrush Color="Black" x:Key="Summary_percent"></SolidColorBrush>
    <SolidColorBrush Color="#25FFAE" x:Key="Summary_trueColor"></SolidColorBrush>
    <SolidColorBrush Color="#FFF7413A" x:Key="Summary_falseColor"></SolidColorBrush>
    <SolidColorBrush Color="#E55090A1" x:Key="Summary_TransactionAndCategoryBackground"></SolidColorBrush>
    <SolidColorBrush Color="#053B50" x:Key="Summary_RightArrow"></SolidColorBrush>
    <SolidColorBrush Color="#FF88CDD9" x:Key="Summary_LeftArrow"></SolidColorBrush>
    
    <SolidColorBrush Color="#FF8AD0DC" x:Key="Summary_firstBlock"></SolidColorBrush>
    <SolidColorBrush Color="#FF40C1D7" x:Key="Summary_secondBlock"></SolidColorBrush>
    <SolidColorBrush Color="#FF82A7ED" x:Key="Summary_thirdBlock"></SolidColorBrush>
    <SolidColorBrush Color="#FF2B43B8" x:Key="Summary_fourthBlock"></SolidColorBrush>

    <!--user pop up-->
    <SolidColorBrush Color="#136688" x:Key="User_background"></SolidColorBrush>
    <SolidColorBrush Color="#FF0A4B65" x:Key="User_onHovered"></SolidColorBrush>
</ResourceDictionary>